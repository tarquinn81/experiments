Config { font = "-*-Fixed-Bold-R-Normal-*-13-*-*-*-*-*-*-*"
       , bgColor = "black"
       , fgColor = "#dd4814"
       , position = TopW L 90
       , commands = [ Run Weather "KPVU" ["-t","
<tempF>F","-L","40","-H","70","--normal","blue","--high","red","--low","darkpurple"]
36000
                    , Run Cpu
["-L","3","-H","50","--normal","green","--high","red"] 10
                    , Run Memory ["-t","Mem: <usedratio>%"] 10
                    , Run Swap [] 10
                    , Run Date "%a %b %_d %l:%M" "date" 10
                    , Run StdinReader
                    , Run CommandReader "/home/quinn/.xmonad/ledmon" "LED"
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ <fc=#0000ff>%LED%</fc> %cpu% |
%memory% * %swap%    <fc=#dd4814>%date%</fc> | %KPVU%"
       }